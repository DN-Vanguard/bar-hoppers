ata, setUserFormData] = useState({ email: '', password: '' });\n  const [validated] = useState(false);\n  const [showAlert, setShowAlert] = useState(false);\n  const [loginUser] = useMutation(LOGIN_USER);\n\n  const [open, setOpen] = React.useState(false);\n  const handleOpen = () => setOpen(true);\n  const handleClose = () => setOpen(false);\n\n  const handleInputChange = (event) => {\n    const { name, value } = event.target;\n    setUserFormData({ ...userFormData, [name]: value });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    // check if form has everything (as per react-bootstrap docs)\n    const form = event.currentTarget;\n    if (form.checkValidity() === false) {\n      event.preventDefault();\n      event.stopPropagation();\n    }\n\n    try {\n      const { data } = await loginUser({\n        variables: { ...userFormData }\n      })\n      Auth.login(data.login.token);\n    } catch (err) {\n      console.error(err);\n      setShowAlert(true);\n    }\n\n    setUserFormData({\n      username: '',\n      email: '',\n      password: '',\n    });\n  };\n\n  return (\n    <>\n      <Button variant=\"contained\" onClick={handleOpen}>Login</Button>\n      <Modal open={open} onClose={handleClose}>\n        <Box sx={style}\n          component=\"form\"\n          noValidate\n          validated={validated}\n          onSubmit={handleFormSubmit}\n        >\n          {/* <Alert dismissible onClose={() => setShowAlert(false)} show={showAlert} variant='danger'>\n            Something went wrong with your login credentials!\n          </Alert> */}\n          <FormControl>\n            <InputLabel htmlFor='email'>Email</InputLabel>\n            <Input\n              type='email'\n              name='email'\n              onChange={handleInputChange}\n              value={userFormData.email}\n              required\n            />\n            {/* <Form.Control.Feedback type='invalid'>Email is required!</Form.Control.Feedback> */}\n          </FormControl>\n\n          <FormControl>\n            <InputLabel htmlFor='password'>Password</InputLabel>\n            <Input\n              type='password'\n              name='password'\n              onChange={handleInputChange}\n              value={userFormData.password}\n              required\n            />\n            {/* <Form.Control.Feedback type='invalid'>Password is required!</Form.Control.Feedback> */}\n          </FormControl>\n          <Button\n            disabled={!(userFormData.email && userFormData.password)}\n            type='submit'\n            variant='success'>\n            Submit\n          </Button>\n        </Box>\n      </Modal>\n    </>\n  );\n};\n\nexport default LoginForm;\n","/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/components/SignupForm.js",["127","128"],"import React, { useState } from 'react';\nimport { ADD_USER } from '../utils/mutations';\nimport { useMutation } from '@apollo/client';\nimport Auth from '../utils/auth';\nimport { Modal, FormControl, InputLabel, Input, Alert, Button } from '@mui/material';\nimport { Box } from '@mui/system';\n\nconst style = {\n  position: 'absolute',\n  top: '50%',\n  left: '50%',\n  transform: 'translate(-50%, -50%)',\n  width: 400,\n  bgcolor: '#FFF',\n  border: '2px solid #000',\n  boxShadow: 24,\n  p: 4,\n};\n\nconst SignupForm = () => {\n  // set initial form state\n  const [userFormData, setUserFormData] = useState({ username: '', email: '', password: '' });\n  // set state for form validation\n  const [validated] = useState(false);\n  // set state for alert\n  const [showAlert, setShowAlert] = useState(false);\n  const [addUser] = useMutation(ADD_USER);\n\n  const [open, setOpen] = React.useState(false);\n  const handleOpen = () => setOpen(true);\n  const handleClose = () => setOpen(false);\n\n  const handleInputChange = (event) => {\n\n    const { name, value } = event.target;\n    setUserFormData({ ...userFormData, [name]: value });\n    console.log(name, value);\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    // check if form has everything (as per react-bootstrap docs)\n    const form = event.currentTarget;\n    if (form.checkValidity() === false) {\n      event.preventDefault();\n      event.stopPropagation();\n    }\n\n    try {\n      const { data: info } = await addUser({\n        variables: { ...userFormData }\n      });\n      Auth.login(info.addUser.token);\n    } catch (err) {\n      console.error(err);\n      setShowAlert(true);\n    }\n\n    setUserFormData({\n      username: '',\n      email: '',\n      password: '',\n    });\n  };\n\n  return (\n    <>\n      <Button variant=\"contained\" onClick={handleOpen}>Sign Up</Button>\n      <Modal open={open} onClose={handleClose}>\n      {/* This is needed for the validation functionality above */}\n        <Box sx={style}\n          component=\"form\"\n          noValidate\n          validated={validated}\n          onSubmit={handleFormSubmit}\n        >\n          {/* show alert if server response is bad */}\n          {/* <Alert\n            dismissible\n            onClose={() => setShowAlert(false)} show={showAlert} variant='danger'>\n            Something went wrong with your signup!\n          </Alert> */}\n\n          <FormControl>\n            <InputLabel htmlFor='username'>Username</InputLabel>\n            <Input\n              error\n              type='text'\n              name='username'\n              onChange={handleInputChange}\n              value={userFormData.username}\n              required\n            />\n            {/* <FormControl.Feedback type='invalid'>Username is required!</FormControl.Feedback> */}\n          </FormControl>\n\n          <FormControl>\n            <InputLabel htmlFor='email'>Email</InputLabel>\n            <Input\n              error\n              type='email'\n              name='email'\n              onChange={handleInputChange}\n              value={userFormData.email}\n              required\n            />\n            {/* <FormControl.Feedback type='invalid'>Email is required!</FormControl.Feedback> */}\n          </FormControl>\n\n          <FormControl>\n            <InputLabel htmlFor='password'>Password</InputLabel>\n            <Input\n              error\n              type='password'\n              name='password'\n              onChange={handleInputChange}\n              value={userFormData.password}\n              required\n            />\n            {/* <FormControl.Feedback type='invalid'>Password is required!</FormControl.Feedback> */}\n          </FormControl>\n          <Button\n            disabled={!(userFormData.username && userFormData.email && userFormData.password)}\n            type='submit'\n            variant='success'>\n            Submit\n          </Button>\n        </Box>\n      </Modal>\n    </>\n  );\n};\n\nexport default SignupForm;\n","/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/components/DrinkDisplay.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/utils/API.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/utils/mutations.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/components/pages/DrinkDetail.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/utils/localStorage.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/utils/queries.js",[],"/Users/samnielsen/Bootcamp-gits/bar-hoppers/client/src/components/SavedDrinks.js",[],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","severity":1,"message":"134","line":2,"column":8,"nodeType":"135","messageId":"136","endLine":2,"endColumn":12},{"ruleId":"133","severity":1,"message":"137","line":7,"column":17,"nodeType":"135","messageId":"136","endLine":7,"endColumn":22},{"ruleId":"133","severity":1,"message":"138","line":24,"column":10,"nodeType":"135","messageId":"136","endLine":24,"endColumn":19},{"ruleId":"133","severity":1,"message":"